<!DOCTYPE html>
<html>
<head>
  <title>Moneyspent</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<div class="navigation">
  <ul class="hmenu">
    <%= render 'shared/menu' %>
  </ul>
</div>
<%- flash.each do |name, msg| -%>
    <%= content_tag :div, msg, :id => "flash_#{name}" if msg.is_a?(String) %>
<%- end -%>
<%= yield %>

</body>
</html>
